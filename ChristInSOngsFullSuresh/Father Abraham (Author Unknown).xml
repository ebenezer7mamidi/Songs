<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.1" modifiedIn="OpenLP 3.1.1" modifiedDate="2023-01-09T20:17:45">
  <properties>
    <titles>
      <title>Father Abraham</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Father Abraham had many sons<br/>and many sons had father Abraham<br/>and I am one of them<br/>and so are you<br/>so let's just praise The Lord</lines>
    </verse>
    <verse name="c1a">
      <lines>RIGHT ARM!<br/>(lift right arm upwards)</lines>
    </verse>
    <verse name="c1b">
      <lines>LEFT ARM!<br/>(lift left arm upwards)</lines>
    </verse>
    <verse name="c1c">
      <lines>RIGHT FOOT!<br/>(lift right foot upwards)</lines>
    </verse>
    <verse name="c1d">
      <lines>LEFT FOOT!<br/>(lift left foot upwards)</lines>
    </verse>
    <verse name="c1e">
      <lines>TURN AROUND!<br/>(spin in a circle)</lines>
    </verse>
    <verse name="c1f">
      <lines>SIT DOWN!<br/>(sit down)</lines>
    </verse>
  </lyrics>
</song>
