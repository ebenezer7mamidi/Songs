    <titles>
      <title>CB 83 Hosanna! Hosanna!</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author>Unknown</author>
    </authors>
    <songbooks>
      <songbook name="DCCF Chorus Book" entry="83"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>HOSANNA! HOSANNA!<br/>Blessed is the Son of David!<br/>Hosanna! Hosanna!<br/>That cometh in the name of the Lord!</lines>
    </verse>
    <verse name="v2">
      <lines>Behold! Thy King cometh unto thee.<br/>(Repeat)</lines>
    </verse>
    <verse name="v3">
      <lines>Behold! He reigns for evermore.<br/>(Repeat)</lines>
    </verse>
  </lyrics>
</song>
